<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contractCollector>
    <genericItem>
        <nodeName>ore.crushed.copper.coarse</nodeName>
        <oreDict>crushedCopper</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>ore.crushed.copper.fine</nodeName>
        <oreDict>crushedCopper</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>ore.crushed.copper.finer</nodeName>
        <oreDict>crushedCopper</oreDict>
    </genericItem>
    <genericHOEFluid>
        <nodeName>fluid.froth.copper.rougher</nodeName>
    </genericHOEFluid>
    <genericItem>
        <nodeName>froth.copper.rougher.dust</nodeName>
    </genericItem>
    <genericHOEFluid>
        <nodeName>fluid.froth.copper.final</nodeName>
    </genericHOEFluid>
    <genericItem>
        <nodeName>froth.copper.final.dust</nodeName>
    </genericItem>
    <genericHOEFluid>
        <nodeName>fluid.froth.copper.tailings</nodeName>
    </genericHOEFluid>
    <genericHOEFluid>
        <nodeName>fluid.mixture.frothering</nodeName>
    </genericHOEFluid>
    <genericItem>
        <nodeName>ingot.roasted.copper.ore</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>ingot.copper.pig</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>ingot.copper</nodeName>
        <oreDict>ingotCopper</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>slag.rich.sulfidic.iron</nodeName>
    </genericItem>
    <rigidProcess>
        <nodeName>crush.ore.copper</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <oreDict>oreCopper</oreDict>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.copper.coarse</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>crush.ore.crushed.copper.coarse</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <nodeName>ore.crushed.copper.coarse</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.copper.fine</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Fun fact: it DECREASES efficiency</comment>
        <nodeName>crush.ore.crushed.copper.fine</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <nodeName>ore.crushed.copper.fine</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.copper.finer</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Mixes Frothing Mixture</comment>
        <nodeName>mix.fluid.mixture.frothering</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.MixerIO</IOPackage>
        <in>
            <nodeName>fluid.oil.wheat.germ</nodeName>
            <count>100</count>
        </in>
        <in>
            <nodeName>fluid.water</nodeName>
            <count>600</count>
        </in>
        <in>
            <oreDict>quicklime</oreDict>
            <count>5</count>
        </in>
        <out>
            <nodeName>fluid.mixture.frothering</nodeName>
            <count>1000</count>
        </out>
        <power>100</power>
        <time>25</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Frothing: Low efficiency, High energy loss, Medium resource gain</comment>
        <nodeName>froth.ore.crushed.copper.coarse</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.FrothCellIO</IOPackage>
        <in>
            <nodeName>ore.crushed.copper.coarse</nodeName>
            <count>10</count>
        </in>
        <in>
            <nodeName>fluid.mixture.frothering</nodeName>
            <count>250</count>
        </in>
        <in>
            <nodeName>fluid.air.gas</nodeName>
            <count>1000</count>
        </in>
        <out>
            <nodeName>fluid.froth.copper.rougher</nodeName>
            <count>50</count>
        </out>
        <out>
            <nodeName>fluid.froth.copper.tailings</nodeName>
            <count>30</count>
        </out>
        <out>
            <nodeName>fluid.air.gas</nodeName>
            <count>950</count>
        </out>
        <power>100</power>
        <time>250</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Frothing: High efficiency, Low energy loss, High resource gain</comment>
        <nodeName>froth.ore.crushed.copper.fine</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.FrothCellIO</IOPackage>
        <in>
            <nodeName>ore.crushed.copper.fine</nodeName>
            <count>10</count>
        </in>
        <in>
            <nodeName>fluid.mixture.frothering</nodeName>
            <count>250</count>
        </in>
        <in>
            <nodeName>fluid.air.gas</nodeName>
            <count>1000</count>
        </in>
        <out>
            <nodeName>fluid.froth.copper.rougher</nodeName>
            <count>80</count>
        </out>
        <out>
            <nodeName>fluid.froth.copper.tailings</nodeName>
            <count>20</count>
        </out>
        <out>
            <nodeName>fluid.air.gas</nodeName>
            <count>950</count>
        </out>
        <power>100</power>
        <time>250</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Frothing: Low efficiency, Low energy loss, Low resource gain</comment>
        <nodeName>froth.ore.crushed.copper.finer</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.FrothCellIO</IOPackage>
        <in>
            <nodeName>ore.crushed.copper.finer</nodeName>
            <count>10</count>
        </in>
        <in>
            <nodeName>fluid.mixture.frothering</nodeName>
            <count>250</count>
        </in>
        <in>
            <nodeName>fluid.air.gas</nodeName>
            <count>1000</count>
        </in>
        <out>
            <nodeName>fluid.froth.copper.rougher</nodeName>
            <count>50</count>
        </out>
        <out>
            <nodeName>fluid.froth.copper.tailings</nodeName>
            <count>10</count>
        </out>
        <out>
            <nodeName>fluid.air.gas</nodeName>
            <count>950</count>
        </out>
        <power>100</power>
        <time>250</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Frothing: Cleaning</comment>
        <nodeName>froth.froth.copper.rougher.dust</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.FrothCellIO</IOPackage>
        <in>
            <nodeName>froth.copper.rougher.dust</nodeName>
            <count>10</count>
        </in>
        <in>
            <nodeName>fluid.mixture.frothering</nodeName>
            <count>250</count>
        </in>
        <in>
            <nodeName>fluid.air.gas</nodeName>
            <count>1000</count>
        </in>
        <out>
            <nodeName>fluid.froth.copper.final</nodeName>
            <count>80</count>
        </out>
        <out>
            <nodeName>fluid.froth.copper.tailings</nodeName>
            <count>10</count>
        </out>
        <out>
            <nodeName>fluid.air.gas</nodeName>
            <count>950</count>
        </out>
        <power>100</power>
        <time>250</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Frothing: Salvaging</comment>
        <nodeName>froth.froth.copper.tailings</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.FrothCellIO</IOPackage>
        <in>
            <nodeName>fluid.froth.copper.tailings</nodeName>
            <count>100</count>
        </in>
        <in>
            <nodeName>fluid.air.gas</nodeName>
            <count>1000</count>
        </in>
        <out>
            <nodeName>fluid.froth.copper.rougher</nodeName>
            <count>50</count>
        </out>
        <out>
            <nodeName>fluid.froth.copper.tailings</nodeName>
            <count>10</count>
        </out>
        <out>
            <nodeName>fluid.air.gas</nodeName>
            <count>950</count>
        </out>
        <power>100</power>
        <time>250</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>crush.froth.copper.rougher</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <nodeName>fluid.froth.copper.rougher</nodeName>
            <count>100</count>
        </in>
        <out>
            <nodeName>froth.copper.rougher.dust</nodeName>
            <count>10</count>
        </out>
        <power>500</power>
        <time>50</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>crush.froth.copper.final</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <nodeName>fluid.froth.copper.final</nodeName>
            <count>100</count>
        </in>
        <out>
            <nodeName>froth.copper.final.dust</nodeName>
            <count>10</count>
        </out>
        <power>500</power>
        <time>50</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>heat.froth.copper.final.dust</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>froth.copper.final.dust</nodeName>
            <count>1</count>
        </in>
        <in>
            <nodeName>fluid.air.gas</nodeName>
            <count>100</count>
        </in>
        <out>
            <nodeName>ingot.roasted.copper.ore</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>hirm.ingot.roasted.copper.ore</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiResistantMixerIO</IOPackage>
        <in>
            <nodeName>ingot.roasted.copper.ore</nodeName>
            <count>3</count>
        </in>
        <in>
            <nodeName>fluid.oxygen</nodeName>
            <count>250</count>
        </in>
        <out>
            <nodeName>ingot.copper.pig</nodeName>
            <count>1</count>
        </out>
        <out>
            <nodeName>slag.rich.sulfidic.iron</nodeName>
            <count>1</count>
        </out>
        <out>
            <nodeName>fluid.sulfurdioxide</nodeName>
            <count>50</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>heat.ingot.copper.pig</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>ingot.copper.pig</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>ingot.copper</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
</contractCollector>
