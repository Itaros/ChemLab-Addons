<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contractCollector>
    <genericItem>
        <nodeName>ore.crushed.tin</nodeName>
        <oreDict>crushedTin</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>ore.crushed.tin.rich</nodeName>
    </genericItem>
    <genericHOEFluid>
        <nodeName>fluid.froth.tin</nodeName>
    </genericHOEFluid>
    <genericHOEFluid>
        <nodeName>fluid.froth.tin.tailings</nodeName>
    </genericHOEFluid>
    <genericItem>
        <nodeName>froth.tin.dry</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>dust.tin.rough</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>dust.tin.final</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>clump.tin</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>slag.tin.primary</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>slag.tin.secondary</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>ingot.tin</nodeName>
        <oreDict>ingotTin</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>hardhead.tin</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>slag.waste</nodeName>
    </genericItem>
    <rigidProcess>
        <nodeName>crush.ore.tin</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <oreDict>oreTin</oreDict>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.tin</nodeName>
            <count>2</count>
        </out>
        <out>
            <nodeName>chemlab:dust.stone</nodeName>
            <count>2</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>grav.crushed.tin</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.GravMagIO</IOPackage>
        <in>
            <nodeName>ore.crushed.tin</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.tin.rich</nodeName>
            <count>1</count>
        </out>
        <out>
            <nodeName>chemlab:dust.stone</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>froth.ore.crushed.tin.rich</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.FrothCellIO</IOPackage>
        <in>
            <nodeName>ore.crushed.tin.rich</nodeName>
            <count>10</count>
        </in>
        <in>
            <nodeName>fluid.mixture.frothering</nodeName>
            <count>250</count>
        </in>
        <in>
            <nodeName>fluid.air.gas</nodeName>
            <count>1000</count>
        </in>
        <out>
            <nodeName>fluid.froth.tin</nodeName>
            <count>50</count>
        </out>
        <out>
            <nodeName>fluid.froth.tin.tailings</nodeName>
            <count>30</count>
        </out>
        <out>
            <nodeName>fluid.air.gas</nodeName>
            <count>950</count>
        </out>
        <power>100</power>
        <time>250</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>dry.crushed.tin</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>fluid.froth.tin</nodeName>
            <count>50</count>
        </in>
        <out>
            <nodeName>froth.tin.dry</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>centfg.froth.tin.dry</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CentrifugalExtractorIO</IOPackage>
        <in>
            <nodeName>froth.tin.dry</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>dust.tin.rough</nodeName>
            <count>1</count>
        </out>
        <out>
            <nodeName>chemlab:dust.stone</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>grav.dust.tin.rough</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.GravMagIO</IOPackage>
        <in>
            <nodeName>dust.tin.rough</nodeName>
            <count>10</count>
        </in>
        <out>
            <nodeName>dust.tin.final</nodeName>
            <count>9</count>
        </out>
        <out>
            <nodeName>ore.dust.iron.lowgrade</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <comment>First pass</comment>
        <nodeName>heat.dust.tin.final</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>dust.tin.final</nodeName>
            <count>10</count>
        </in>
        <in>
            <oreDict>quicklime</oreDict>
            <count>1</count>
        </in>
        <in>
            <nodeName>charcoal.pellet</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>slag.tin.primary</nodeName>
            <count>7</count>
        </out>
        <out>
            <nodeName>clump.tin</nodeName>
            <count>3</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Second pass</comment>
        <nodeName>heat.slag.tin.primary</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>slag.tin.primary</nodeName>
            <count>10</count>
        </in>
        <in>
            <oreDict>quicklime</oreDict>
            <count>1</count>
        </in>
        <in>
            <nodeName>charcoal.pellet</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>slag.tin.secondary</nodeName>
            <count>3</count>
        </out>
        <out>
            <nodeName>clump.tin</nodeName>
            <count>7</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Third pass</comment>
        <nodeName>heat.slag.tin.secondary</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>slag.tin.secondary</nodeName>
            <count>10</count>
        </in>
        <in>
            <oreDict>quicklime</oreDict>
            <count>1</count>
        </in>
        <in>
            <nodeName>charcoal.pellet</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>hardhead.tin</nodeName>
            <count>3</count>
        </out>
        <out>
            <nodeName>slag.waste</nodeName>
            <count>5</count>
        </out>
        <out>
            <nodeName>clump.tin</nodeName>
            <count>2</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>heat.clump.tin</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>clump.tin</nodeName>
            <count>2</count>
        </in>
        <out>
            <nodeName>ingot.tin</nodeName>
            <count>1</count>
        </out>
        <power>500</power>
        <time>50</time>
    </rigidProcess>
</contractCollector>
