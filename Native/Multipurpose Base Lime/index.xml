<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contractCollector>
    <chemicalItem>
        <nodeName>ore.crushed.limestone</nodeName>
        <oreDict>crushedLimestone</oreDict>
        <composition>
            <compound>CaCO3</compound>
            <moles>1</moles>
        </composition>
    </chemicalItem>
    <chemicalItem>
        <nodeName>dust.calcbicarbon</nodeName>
        <oreDict>quicklime</oreDict>
        <composition>
            <compound>CaH2C2O6</compound>
            <moles>1</moles>
        </composition>
    </chemicalItem>
    <chemicalItem>
        <nodeName>dust.calchydrox</nodeName>
        <composition>
            <compound>Ca(OH)2</compound>
            <moles>1</moles>
        </composition>
    </chemicalItem>
    <rigidProcess>
        <nodeName>heat.ore.crushed.limestone</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>ore.crushed.limestone</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>dust.calcbicarbon</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>cat.dust.calcbicarbon</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CatalyticTankIO</IOPackage>
        <in>
            <nodeName>dust.calchydrox</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.limestone</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>cat.dust.calcbicarbon</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CatalyticTankIO</IOPackage>
        <in>
            <nodeName>dust.calchydrox</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.limestone</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>crush.ore.limestone</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <oreDict>limestone</oreDict>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.limestone</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
</contractCollector>
