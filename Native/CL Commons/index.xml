<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contractCollector>
    <HOEChemicalCompound>
        <stoichiometric>C</stoichiometric>
        <classicalName>Carbon</classicalName>
    </HOEChemicalCompound>
    <HOEChemicalCompound>
        <stoichiometric>O2</stoichiometric>
        <classicalName>Oxygen</classicalName>
    </HOEChemicalCompound>
    <HOEChemicalCompound>
        <stoichiometric>CO2</stoichiometric>
        <classicalName>Carbon Dioxide</classicalName>
    </HOEChemicalCompound>
    <HOEChemicalReaction>
        <equation>C + O2 -> CO2</equation>
    </HOEChemicalReaction>
    <genericItem>
        <nodeName>clump.iron.bloom</nodeName>
    </genericItem>
    <genericHOEFluid>
        <nodeName>fluid.air.gas</nodeName>
    </genericHOEFluid>
    <genericHOEFluid>
        <nodeName>fluid.air.compressed</nodeName>
    </genericHOEFluid>
    <genericHOEFluid>
        <nodeName>fluid.air.liquid</nodeName>
    </genericHOEFluid>
    <genericHOEFluid>
        <nodeName>fluid.oxygen</nodeName>
    </genericHOEFluid>
    <genericHOEFluid>
        <nodeName>fluid.nitrogen</nodeName>
    </genericHOEFluid>
    <rigidProcess>
        <IOPackage>ru.itaros.chemlab.hoe.io.AirCollectorIO</IOPackage>
        <out>
            <nodeName>fluid.air.gas</nodeName>
            <count>50</count>
        </out>
        <power>100</power>
        <time>25</time>
    </rigidProcess>
    <rigidProcess>
        <IOPackage>ru.itaros.chemlab.hoe.io.FluidCompressorIO</IOPackage>
        <in>
            <nodeName>fluid.air.gas</nodeName>
            <count>1000</count>
        </in>
        <out>
            <nodeName>fluid.air.compressed</nodeName>
            <count>10</count>
        </out>
        <power>1000</power>
        <time>50</time>
    </rigidProcess>
    <rigidProcess>
        <IOPackage>ru.itaros.chemlab.hoe.io.TurboexpanderIO</IOPackage>
        <in>
            <nodeName>fluid.air.compressed</nodeName>
            <count>100</count>
        </in>
        <out>
            <nodeName>fluid.air.liquid</nodeName>
            <count>1000</count>
        </out>
        <power>50</power>
        <time>5</time>
    </rigidProcess>
    <rigidProcess>
        <IOPackage>ru.itaros.chemlab.hoe.io.EvaporationUnitIO</IOPackage>
        <in>
            <nodeName>fluid.air.liquid</nodeName>
            <count>250</count>
        </in>
        <out>
            <nodeName>fluid.nitrogen</nodeName>
            <count>190</count>
        </out>
        <out>
            <nodeName>fluid.oxygen</nodeName>
            <count>50</count>
        </out>
        <power>50</power>
        <time>1000</time>
    </rigidProcess>
</contractCollector>