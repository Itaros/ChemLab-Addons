<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contractCollector>
    <genericItem>
        <nodeName>ore.crushed.iron</nodeName>
        <oreDict>crushedIron</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>ore.dust.stone</nodeName>
        <oreDict>dustStone</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>ore.dust.iron.lowgrade</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>ore.dust.ferric.nonmagnetic</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>ore.dust.iron.redshort</nodeName>
    </genericItem>
    <genericItem>
        <nodeName>ore.dust.iron</nodeName>
        <oreDict>dustIron</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>ingot.iron.cast.lowgrade</nodeName>
        <oreDict>ingotIronCastLow</oreDict>
        <oreDict>ingotIronMid</oreDict>
    </genericItem>
    <genericItem>
        <nodeName>clump.iron.wrought</nodeName>
        <oreDict>bloomIron</oreDict>
        <comment>Technically this is iron bloom. The same is produced directly by bloomery</comment>
    </genericItem>
    <genericItem>
        <nodeName>ingot.iron.wrought</nodeName>
        <oreDict>ingotIronWrought</oreDict>
        <oreDict>ingotIronLow</oreDict>
    </genericItem>
    <rigidProcess>
        <nodeName>crush.ore.iron</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <oreDict>oreIron</oreDict>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.crushed.iron</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>heat.crushed.ore.iron</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>ore.crushed.iron</nodeName>
            <count>1</count>
        </in>
        <in>
            <nodeName>fluid.oxygen</nodeName>
            <count>250</count>
        </in>
        <out>
            <nodeName>clump.iron.wrought</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>crush.clump.iron.wrought</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CrusherIO</IOPackage>
        <in>
            <nodeName>clump.iron.wrought</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>ingot.iron.wrought</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>centrifuge.crushed.ore.iron</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.CentrifugalExtractorIO</IOPackage>
        <in>
            <nodeName>ore.crushed.iron</nodeName>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.dust.iron.lowgrade</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>magsep.dust.iron.lowgrade</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.GravMagIO</IOPackage>
        <in>
            <nodeName>ore.dust.iron.lowgrade</nodeName>
            <count>2</count>
        </in>
        <out>
            <nodeName>ore.dust.iron.redshort</nodeName>
            <count>1</count>
        </out>
        <out>
            <nodeName>ore.dust.ferric.nonmagnetic</nodeName>
            <count>1</count>
        </out>
        <power>2500</power>
        <time>50</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>heat.dust.iron.lowgrade</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>ore.dust.iron.lowgrade</nodeName>
            <count>1</count>
        </in>
        <in>
            <nodeName>fluid.oxygen</nodeName>
            <count>500</count>
        </in>
        <out>
            <nodeName>clump.iron.wrought</nodeName>
            <count>2</count>
        </out>
        <power>2000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <comment>Removes sulfur from red-short ore dust</comment>
        <nodeName>heat.dust.iron.redshort</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>ore.dust.iron.redshort</nodeName>
            <count>1</count>
        </in>
        <in>
            <oreDict>crushedQuicklime</oreDict>
            <count>1</count>
        </in>
        <out>
            <nodeName>ore.dust.iron</nodeName>
            <count>1</count>
        </out>
        <power>1000</power>
        <time>100</time>
    </rigidProcess>
    <rigidProcess>
        <nodeName>heat.dust.iron</nodeName>
        <IOPackage>ru.itaros.chemlab.hoe.io.HiTFurnaceIO</IOPackage>
        <in>
            <nodeName>ore.dust.iron</nodeName>
            <count>1</count>
        </in>
        <in>
            <nodeName>fluid.oxygen</nodeName>
            <count>500</count>
        </in>
        <out>
            <oreDict>ingotIron</oreDict>
            <count>1</count>
        </out>
        <power>2000</power>
        <time>100</time>
    </rigidProcess>
</contractCollector>
